<h1>Help Infos</h1>

<table>
  <thead>
    <tr>
      <th>Helptext</th>
      <th>Updated by</th>
      <th colspan="3"></th>
    </tr>
  </thead>
<% if @help_infos != nil %>
NOT EMPTY
  <tbody>
    <% @help_infos.each do |help_info| %>
      <tr>
        <td><%= help_info.helptext %></td>
        <td><%= help_info.updated_by %></td>
        <td><%= link_to 'Show', help_info %></td>
        <td><%= link_to 'Edit', edit_help_info_path(help_info) %></td>
        <td><%= link_to 'Destroy', help_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
  <% else %>
    Print if empty help_infos list
  <% end -%>
</table>

<br>

<%= link_to 'New Help Info', new_help_info_path %>
